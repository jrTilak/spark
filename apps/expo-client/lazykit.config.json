{
  "$schema": "https://lazykit.jrtilak.dev/schema/config.json",
  "language": "typescript",
  "separate": true,
  "v": "2.0.2",
  "filenameConvention": {
    "helperFunctions": "kebab-case",
    "reactHooks": "kebab-case"
  },
  "paths": {
    "helperFunctions": "@/lib",
    "reactHooks": "@/hooks"
  },
  "resolve": {
    "alias": {
      "@/lib": "/src/lib",
      "@/hooks": "/src/hooks"
    }
  }
}